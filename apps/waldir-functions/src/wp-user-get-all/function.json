{
    "bindings": [
        {
            "name": "triggerMessage",
            "type": "queueTrigger",
            "queueName": "wp-user-get-all",
            "connection": "wrdsbwaldir_STORAGE",
            "direction": "in"
        },
        {
            "name": "outputBlob",
            "type": "blob",
            "path": "wp-user-get-all/{payload.wpDomain}-{payload.wpSite}-{payload.wpEnvironment}.json",
            "connection": "wrdsbwaldir_STORAGE",
            "direction": "out"
        },
        {
          "name": "invocationPostProcessor",
          "type": "queue",
          "queueName": "invocation-post-processor",
          "connection": "wrdsbwaldir_STORAGE",
          "direction": "out"
        }
    ],
    "scriptFile": "./index.js"
}