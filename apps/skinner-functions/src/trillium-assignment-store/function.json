{
  "bindings": [{
      "name": "triggerMessage",
      "type": "queueTrigger",
      "queueName": "trillium-assignment-store",
      "connection": "wrdsbskinner_STORAGE",
      "direction": "in"
    },
    {
      "name": "recordIn",
      "type": "cosmosDB",
      "databaseName": "skinner",
      "collectionName": "assignments",
      "id": "{payload.id}",
      "partitionKey": "{payload.id}",
      "connectionStringSetting": "wrdsb-skinner_COSMOSDB",
      "direction": "in"
    },
    {
      "name": "recordOut",
      "type": "cosmosDB",
      "databaseName": "skinner",
      "collectionName": "assignments",
      "connectionStringSetting": "wrdsb-skinner_COSMOSDB",
      "direction": "out"
    },
    {
      "name": "invocationPostProcessor",
      "type": "queue",
      "queueName": "invocation-post-processor",
      "connection": "wrdsbskinner_STORAGE",
      "direction": "out"
    }
  ],
  "scriptFile": "./index.js"
}
