{
  "bindings": [{
      "name": "triggerMessage",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "trillium-enrolments-abc-reconcile",
      "connection": "wrdsbskinner_STORAGE"
    },
    {
      "name": "recordsNow",
      "type": "blob",
      "path": "trillium/enrolments-now-sorted-object.json",
      "connection": "wrdsbskinner_STORAGE",
      "direction": "in"
    },
    {
      "name": "cosmosRecords",
      "type": "cosmosDB",
      "databaseName": "skinner",
      "collectionName": "enrolments",
      "sqlQuery": "SELECT * FROM c WHERE startswith(c.id, {alpha}) and c.deleted = false",
      "connectionStringSetting": "",
      "direction": "in"
    },
    {
      "name": "queueStore",
      "type": "queue",
      "queueName": "trillium-enrolment-store",
      "connection": "wrdsbskinner_STORAGE",
      "direction": "out"
    },
    {
      "name": "invocationPostProcessor",
      "type": "queue",
      "queueName": "invocation-post-processor",
      "connection": "wrdsbskinner_STORAGE",
      "direction": "out"
    }
  ],
  "disabled": false,
  "scriptFile": "./index.js"
}
