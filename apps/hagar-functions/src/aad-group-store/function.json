        },
        {
            "name": "recordOut",
            "type": "cosmosDB",
            "databaseName": "hagar",
            "collectionName": "groups",
            "connectionStringSetting": "wrdsb-hagar_COSMOSDB",
            "direction": "out"
        },
        {
            "name": "callbackMessage",
            "type": "queue",
            "queueName": "callbacks",
            "connection": "wrdsbhagar_STORAGE",
            "direction": "out"
        },
        {
            "name": "flynnEvent",
            "type": "queue",
            "queueName": "event-emitter",
            "connection": "wrdsbhagar_STORAGE",
            "direction": "out"
        }

    ],
    "disabled": false,
    "scriptFile": "./index.js"
