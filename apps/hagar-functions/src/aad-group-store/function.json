            "queueName": "aad-group-store",
            "connection": "wrdsbhagar_STORAGE",
            "direction": "in"
        },
        {
            "name": "recordIn",
            "type": "cosmosDB",
            "databaseName": "hagar",
            "collectionName": "groups",
