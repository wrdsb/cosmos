{
  "bindings": [
      {
          "name": "req",
          "type": "httpTrigger",
          "authLevel": "anonymous",
          "methods": [
              "get",
              "post"
          ],
          "direction": "in"
      },
      {
          "name": "res",
          "type": "http",
          "direction": "out"
      },
      {
          "name": "callbackMessage",
          "type": "queue",
          "queueName": "callbacks",
          "connection": "wrdsbviewfinder_STORAGE",
          "direction": "out"
      },
      {
          "name": "flynnEvent",
          "type": "queue",
          "queueName": "event-emitter",
          "connection": "wrdsbviewfinder_STORAGE",
          "direction": "out"
      }
  ],
  "scriptFile": "../dist/ping/index.js"
}