{
    "bindings": [
        {
            "name": "triggerMessage",
            "type": "queueTrigger",
            "direction": "in",
            "queueName": "ipps-people-reconcile",
            "connection": "wrdsbflenderson_STORAGE"
        },
        {
            "name": "peopleNow",
            "type": "blob",
            "path": "ipps/people-now-object.json",
            "connection": "wrdsbflenderson_STORAGE",
            "direction": "in"
        },
        {
            "name": "directoryNow",
            "type": "blob",
            "path": "ipps/directory-now-object.json",
            "connection": "wrdsbflenderson_STORAGE",
            "direction": "in"
        },
        {
            "name": "queueStore",
            "type": "queue",
            "queueName": "ipps-person-store",
            "connection": "wrdsbflenderson_STORAGE",
            "direction": "out"
        },
        {
          "name": "invocationPostProcessor",
          "type": "queue",
          "queueName": "invocation-post-processor",
          "connection": "wrdsbflenderson_STORAGE",
          "direction": "out"
        }
    ],
    "scriptFile": "./index.js"
}
