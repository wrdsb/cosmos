{
  "bindings": [
    {
      "name": "triggerMessage",
      "type": "queueTrigger",
      "queueName": "device-loan-store",
      "connection": "wrdsbquartermaster_STORAGE",
      "direction": "in"
    },
    {
      "name": "recordIn",
      "type": "cosmosDB",
      "databaseName": "quartermaster",
      "collectionName": "device-loans",
      "id" : "{payload.id}",
      "partitionKey": "{payload.id}",
      "connectionStringSetting": "wrdsb-quartermaster_CosmosDB",
      "direction": "in"
    },
    {
      "name": "recordOut",
      "type": "cosmosDB",
      "databaseName": "quartermaster",
      "collectionName": "device-loans",
      "createIfNotExists": false,
      "connectionStringSetting": "wrdsb-quartermaster_CosmosDB",
      "direction": "out"
    },
    {
      "name": "invocationPostProcessor",
      "type": "queue",
      "queueName": "invocation-post-processor",
      "connection": "wrdsbquartermaster_STORAGE",
      "direction": "out"
    }
  ],
  "scriptFile": "./index.js"
}
