{
    "bindings": [
        {
            "name": "req",
            "type": "httpTrigger",
            "authLevel": "anonymous",
            "methods": [
                "post"
            ],
            "direction": "in"
        },
        {
            "name": "res",
            "type": "http",
            "direction": "out"
        },
        {
            "name": "callbackMessage",
            "type": "queue",
            "queueName": "callbacks",
            "connection": "wrdsbigor_STORAGE",
            "direction": "out"
        },
        {
            "name": "flynnEvent",
            "type": "queue",
            "queueName": "event-emitter",
            "connection": "wrdsbigor_STORAGE",
            "direction": "out"
        }
    ],
    "scriptFile": "./index.js"
}