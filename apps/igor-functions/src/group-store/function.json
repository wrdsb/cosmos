{
  "bindings": [
    {
      "name": "triggerMessage",
      "type": "queueTrigger",
      "queueName": "group-store",
      "connection": "wrdsbigor3_STORAGE",
      "direction": "in"
    },
    {
      "name": "recordIn",
      "type": "cosmosDB",
      "databaseName": "igor",
      "collectionName": "groups-groups",
      "id" : "{email}",
      "partitionKey": "{email}",
      "connectionStringSetting": "wrdsb-igor_CosmosDB",
      "direction": "in"
    },
    {
      "name": "recordOut",
      "type": "cosmosDB",
      "databaseName": "igor",
      "collectionName": "groups-groups",
      "createIfNotExists": false,
      "connectionStringSetting": "wrdsb-igor_CosmosDB",
      "direction": "out"
    },
    {
      "name": "invocationPostProcessor",
      "type": "queue",
      "queueName": "invocation-post-processor",
      "connection": "wrdsbigor3_STORAGE",
      "direction": "out"
    }
  ],
  "disabled": false
}
