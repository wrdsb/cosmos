{
    "bindings": [
        {
            "name": "triggerMessage",
            "type": "queueTrigger",
            "queueName": "materialize-person",
            "connection": "wrdsbcodex_STORAGE",
            "direction": "in"
        },
        {
            "name": "flendersonIn",
            "type": "cosmosDB",
            "databaseName": "flenderson",
            "collectionName": "people",
            "id": "{ein}",
            "partitionKey": "{ein}",
            "connectionStringSetting": "wrdsb-flenderson_CosmosDB",
            "direction": "in"
        },
        {
            "name": "skinnerIn",
            "type": "cosmosDB",
            "databaseName": "skinner",
            "collectionName": "staff",
            "id": "{ein}",
            "partitionKey": "{ein}",
            "connectionStringSetting": "wrdsb-skinner_CosmosDB",
            "direction": "in"
        },
        {
            "name": "personStore",
            "type": "queue",
            "queueName": "person-store",
            "connection": "wrdsbcodex_STORAGE",
            "direction": "out"
        },
        {
            "name": "callbackMessage",
            "type": "queue",
            "queueName": "callbacks",
            "connection": "wrdsbcodex_STORAGE",
            "direction": "out"
        },
        {
            "name": "flynnEvent",
            "type": "queue",
            "queueName": "event-emitter",
            "connection": "wrdsbcodex_STORAGE",
            "direction": "out"
        }
    ],
    "scriptFile": "../dist/materialize-person/index.js"
}
